{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "lib": ["DOM"],
      "module": "es6",
      "target": "es5",
      "noUnusedLocals": true,
      // "noEmit": true,
      "removeComments": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "strictNullChecks": true,
      "noEmitOnError": true,
      "allowJs": true,
      "moduleResolution": "node"
    }
  }